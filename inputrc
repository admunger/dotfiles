set enable-meta-key On
set editing-mode emacs
set bell-style none

"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word

TAB:complete

# "\e[Z": menu-complete
# menu-complete with Alt+s
"\es": menu-complete
"\eS": menu-complete-backward
